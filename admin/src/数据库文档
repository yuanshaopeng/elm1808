数据库名称：ele;

后台管理员账号集合:adminList
_id:"",
admin_name:"管理员账号"
admin_pwd:"管理员密码",
admin_jsID:"管理员角色id"
admin_jsName:"管理员角色名称"

后台管理员账户详情：adminOpt
_id:"
admin_ID:"",
admin_name:"",
admin_jsName:"",
admin_tx:"",
admin_tel:"",

后台管理员日志集合：adminLog;
_id:"",
admin_ID:"管理员id"；
doTime:"操作时间"
doSth:"操作内容"

后台管理员角色集合：admin_jsList
_id:"",
js_name:"角色名称"

后台菜单栏集合：admin_menu;
_id:"",
menu_name:"菜单名称“
menu_jsID:"角色id"
children:[
    menu_name:"",
    menu_url:""
]


店铺类别集合  typeList
_id:",
typeName:类别名称,
typeLogo:类别图片,
typeStart:类别开始时间
typeEnd:类别结束时间

店铺集合  shopList
_id:"",
shopName:"店铺名称",
shopLogo:"店铺图片",
shopAddress:"店铺地址",
shopTel:"店铺电话",
shopType:"店铺类别",
shopTypeID:"店铺类别id",
shopOpt:"店铺介绍",
shopRate:"店铺星级",

商品类别集合   goodsType
_id:"",
typeName:"类别名称",
shopID:"店铺id"

商品集合   goodsList
_id:"",
goodsName:"商品名称",
goodsPrice:"商品单价",
goodsLogo:"商品图片",
goodsReta:"商品评分",
typeID:"商品类别id",
shopID:"店铺id",
shopName:"店铺名称"
